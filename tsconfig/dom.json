    "declaration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "module": "commonjs"
  }
}
